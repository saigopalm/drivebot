      "source": [
        "import os\n",
        "from google.colab import files\n",
        "\n",
        "print(\"Upload your .txt files...\")\n",
        "uploaded = files.upload()\n",
        "\n",
        "os.makedirs(\"data\", exist_ok=True)\n",
        "for fname in uploaded:\n",
        "    os.rename(fname, f\"data/{fname}\")"
